#include <iostream>
using namespace std;

int sumArray1(int arr[], int n) {
    int s = 0;
    for(int i=0;i<n;i++) s += arr[i];
    return s;
}

int sumArray2(int arr[], int n) {
    int s = 0;
    for(int i=0;i<n;i++) s += arr[i]; // същото като sumArray1
    return s;
}

int main() {
    int arr[3] = {1,2,3};
    cout << sumArray1(arr, 3) << " " << sumArray2(arr, 3) << endl;
}
